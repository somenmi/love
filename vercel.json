{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/client/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/client/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/client/index.html"
        }
    ]
}